<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/google_map_fragment">

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="100sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/map_lasek_btn"
        android:layout_width="50sp"
        android:layout_height="25sp"
        android:text="라섹"
        android:textSize="15sp"
        android:background="@drawable/map_lasek_btn"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/map_lasik_btn"
        android:layout_width="50sp"
        android:layout_height="25sp"
        android:text="라식"
        android:textSize="15sp"
        android:background="@drawable/map_lasik_btn"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@id/map_lasek_btn"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/map_smile_btn"
        android:layout_width="90sp"
        android:layout_height="25sp"
        android:text="스마일라식"
        android:textSize="15sp"
        android:background="@drawable/map_smile_btn"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@id/map_lasik_btn"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/map_lens_btn"
        android:layout_width="90sp"
        android:layout_height="25sp"
        android:text="렌즈삽입술"
        android:textSize="15sp"
        android:background="@drawable/map_lens_btn"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@id/map_smile_btn"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/map_ophthalmology_btn"
        android:layout_width="50sp"
        android:layout_height="25sp"
        android:text="안과"
        android:textSize="15sp"
        android:background="@drawable/map_ophthalmology_btn"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@id/map_lens_btn"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/map_list_btn"
        android:layout_width="180dp"
        android:layout_height="25sp"
        android:text="목록보기"
        android:textSize="15sp"
        android:background="@drawable/map_list_btn"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="30dp"/>




</androidx.constraintlayout.widget.ConstraintLayout>
