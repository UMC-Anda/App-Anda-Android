<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/symptomFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/dictionary_symptom_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50sp"
        android:layout_marginStart="30sp"
        android:text="@string/dictionary_symptom_title"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/dictionary_eyes_underbar"
        app:layout_constraintStart_toStartOf="@id/dictionary_symptom_tv"
        app:layout_constraintEnd_toEndOf="@id/dictionary_symptom_tv"
        app:layout_constraintBottom_toBottomOf="@id/dictionary_symptom_tv"/>

    <ImageView
        android:id="@+id/dictionary_symptom_nearsighted_category_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:src="@drawable/dictionary_category_background"
        app:layout_constraintTop_toBottomOf="@+id/dictionary_symptom_tv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
        android:id="@+id/dictionary_symptom_nearsighted_category_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="근시"
        android:textColor="#282F20"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/dictionary_symptom_nearsighted_category_iv"
        app:layout_constraintEnd_toEndOf="@+id/dictionary_symptom_nearsighted_category_iv"
        app:layout_constraintTop_toTopOf="@+id/dictionary_symptom_nearsighted_category_iv"
        app:layout_constraintBottom_toBottomOf="@+id/dictionary_symptom_nearsighted_category_iv"/>

    <ImageView
        android:id="@+id/dictionary_symptom_astigmatism_category_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:src="@drawable/dictionary_category_background"
        app:layout_constraintTop_toBottomOf="@+id/dictionary_symptom_nearsighted_category_iv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
        android:id="@+id/dictionary_symptom_astigmatism_category_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="난시"
        android:textColor="#282F20"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/dictionary_symptom_astigmatism_category_iv"
        app:layout_constraintEnd_toEndOf="@+id/dictionary_symptom_astigmatism_category_iv"
        app:layout_constraintTop_toTopOf="@+id/dictionary_symptom_astigmatism_category_iv"
        app:layout_constraintBottom_toBottomOf="@+id/dictionary_symptom_astigmatism_category_iv"/>

    <ImageView
        android:id="@+id/dictionary_symptom_farsighted_category_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:src="@drawable/dictionary_category_background"
        app:layout_constraintTop_toBottomOf="@+id/dictionary_symptom_astigmatism_category_iv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
        android:id="@+id/dictionary_symptom_farsighted_category_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="원시"
        android:textColor="#282F20"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/dictionary_symptom_farsighted_category_iv"
        app:layout_constraintEnd_toEndOf="@+id/dictionary_symptom_farsighted_category_iv"
        app:layout_constraintTop_toTopOf="@+id/dictionary_symptom_farsighted_category_iv"
        app:layout_constraintBottom_toBottomOf="@+id/dictionary_symptom_farsighted_category_iv"/>

    <ImageView
        android:id="@+id/dictionary_symptom_presbyopia_category_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:src="@drawable/dictionary_category_background"
        app:layout_constraintTop_toBottomOf="@+id/dictionary_symptom_farsighted_category_iv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
        android:id="@+id/dictionary_symptom_presbyopia_category_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="노안"
        android:textColor="#282F20"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/dictionary_symptom_presbyopia_category_iv"
        app:layout_constraintEnd_toEndOf="@+id/dictionary_symptom_presbyopia_category_iv"
        app:layout_constraintTop_toTopOf="@+id/dictionary_symptom_presbyopia_category_iv"
        app:layout_constraintBottom_toBottomOf="@+id/dictionary_symptom_presbyopia_category_iv"/>

</androidx.constraintlayout.widget.ConstraintLayout>